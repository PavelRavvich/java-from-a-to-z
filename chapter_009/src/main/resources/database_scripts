#Add user in table users.
add=\
  INSERT INTO users (id, username, login, email, create_date)\
  VALUES (DEFAULT, (?), (?), (?), now()::timestamp)


#Delete user from table users.
delete=DELETE FROM users WHERE id = (?)


#Get user by id.
get=SELECT * FROM users WHERE id = (?)


#Update old user.
update=UPDATE users SET  username = (?), login = (?), email = (?) WHERE id = (?)


#Get all users.
getAll=SELECT * FROM users


#Check user exist
exist=SELECT count(*) FROM users WHERE username = (?) AND email = (?)